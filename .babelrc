{
  "presets": [
    "env",
    "stage-0"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    [
      "babel-plugin-transform-builtin-extend", {
        "globals": ["Error"]
      }
    ]
  ],
  "env": {
    "test": {
      "sourceMaps": "inline",
      "retainLines": true
    }
  }
}
